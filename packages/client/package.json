{
  "name": "@munode/client",
  "version": "1.0.0",
  "description": "Headless Mumble client for MuNode - supports HTTP API, WebSocket, and Node.js API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "mumble",
    "client",
    "headless",
    "bot",
    "api",
    "websocket"
  ],
  "author": "MuNode",
  "license": "MIT",
  "dependencies": {
    "@munode/common": "workspace:*",
    "@munode/protocol": "workspace:*",
    "fastify": "^4.25.0",
    "ws": "^8.14.0",
    "@discordjs/opus": "^0.9.0",
    "fluent-ffmpeg": "^2.1.2",
    "long": "^5.2.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "@types/fluent-ffmpeg": "^2.1.24",
    "typescript": "^5.3.0",
    "vitest": "^1.4.0"
  }
}
