{
  "name": "@munode/protocol",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "generate:proto": "protoc --proto_path=./proto --js_out=import_style=commonjs,binary:./src/generated/proto --ts_out=service=grpc-node:./src/generated/proto ./proto/Mumble.proto",
    "test": "vitest run"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.10.1",
    "@munode/common": "workspace:*",
    "google-protobuf": "^3.21.4",
    "msgpackr": "^1.11.5",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.12",
    "@types/node": "^22.0.0",
    "@types/ws": "^8.18.1",
    "protoc-gen-ts": "^0.8.7",
    "typescript": "^5.9.3"
  }
}
