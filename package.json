{
  "name": "munode",
  "version": "0.1.0",
  "private": true,
  "description": "MuNode Mumble Server - Node.js Implementation",
  "type": "module",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "dev:hub": "pnpm --filter hub-server dev",
    "dev:edge": "pnpm --filter edge-server dev",
    "start:hub": "pnpm --filter hub-server start",
    "start:edge": "pnpm --filter edge-server start",
    "test": "pnpm -r test",
    "test:watch": "pnpm -r test:watch",
    "test:coverage": "pnpm -r test:coverage",
    "test:integration": "vitest run --config vitest.config.integration.ts",
    "test:integration:watch": "vitest watch --config vitest.config.integration.ts",
    "test:integration:ui": "vitest --ui --config vitest.config.integration.ts",
    "test:integration:manual": "tsx tests/integration/test-integration.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "generate:proto": "pnpm --filter protocol generate:proto",
    "generate:cert": "pnpm --filter cli generate:cert",
    "clean": "pnpm -r clean && rm -rf node_modules"
  },
  "keywords": [
    "mumble",
    "voip",
    "voice-chat",
    "typescript",
    "distributed"
  ],
  "author": "MuNode Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5",
    "vitest": "^1.4.0"
  },
  "packageManager": "pnpm@8.15.4",
  "dependencies": {
    "@grpc/grpc-js": "^1.14.1",
    "@grpc/proto-loader": "^0.8.0",
    "@munode/common": "workspace:*"
  }
}
